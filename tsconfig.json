{
  "compilerOptions": {
    "target": "es2016",
    "jsx": "react",
    "module": "commonjs",
    "rootDir": "./",
    "moduleResolution": "node",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "sourceMap": true,
    "paths": {
      "*": ["./@types/*"]
    },
    "typeRoots": ["./@types/*", "./node_modules/@types"]
  }
}
